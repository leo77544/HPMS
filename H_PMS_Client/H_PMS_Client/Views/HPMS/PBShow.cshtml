
<div class="main-content" style="margin-left:0px">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="#">首页</a>
            </li>
            <li class="active">房源管理</li>
            <li class="active">发布记录</li>
        </ul><!-- .breadcrumb -->
    </div>

    <div class="page-content">
        <div class="row">
            <div class="col-xs-12">
                <div class="table-responsive">
                    <table id="sample-table-1" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th class="center">
                                    <label>
                                        <input type="checkbox" class="ace" />
                                        <span class="lbl"></span>
                                    </label>
                                </th>
                                <th>ID</th>
                                <th>车位编号</th>
                                <th>车位面积</th>
                                <th>车位类型</th>
                                <th>车位价钱</th>
                                <th>车位位置</th>
                                <th>车位状态</th>
                                <th>操作</th>
                            </tr>
                        </thead>

                        <tbody>

                            @foreach (var item in ViewBag.list)
                            {
                            <tr>
                                <td class="center">
                                    <label>
                                        <input type="checkbox" class="ace" />
                                        <span class="lbl"></span>
                                    </label>
                                </td>

                                <td>@item.PBId</td>
                                <td>@item.PBNumber</td>
                                <td>@item.PBArea</td>
                                <td>@item.PBType</td>
                                <td>@item.PBPrice</td>
                                <td>@item.PBPlace</td>
                                <td>@item.Remark</td>
                                <td class="ui-pg-button ui-corner-all" title="">
                                    <div class="ui-pg-div" style="display:inline-block"><span class="ui-icon icon-pencil blue"></span></div>
                                    <div class="ui-pg-div" style="display:inline-block"><span class="ui-icon icon-trash red"></span></div>
                                </td>
                            </tr>

                            }


                        </tbody>
                    </table>
                </div><!-- /.table-responsive -->
            </div><!-- /span -->
        </div><!-- /row -->
    </div><!-- /.page-content -->
</div><!-- /.main-content -->